{
  "name": "eco-swift-backend",
  "version": "1.0.0",
  "description": "Eco Swift E-commerce Backend",
  "main": "public/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node public/index.js",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:watch": "graphql-codegen --config codegen.yml --watch",
    "seed:admin": "ts-node --transpile-only src/scripts/seedAdmin.ts",
    "seed:categories": "ts-node --transpile-only src/scripts/seedCategories.ts"
  },
  "keywords": [
    "graphql",
    "express",
    "mongodb",
    "ecommerce"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/schema": "^10.0.29",
    "@types/ws": "^8.18.1",
    "apollo-server-express": "^3.12.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-subscriptions": "^3.0.0",
    "graphql-ws": "^6.0.6",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "winston": "^3.18.3",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/winston": "^2.4.4",
    "glob": "^10.3.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
